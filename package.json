{
	"name": "@freddieridell/hyperpipe",
	"version": "0.1.0",
	"public": true,
	"description": "uses hyperswarm to share stdin and stdout between two peers",
	"main": "index.js",
	"homepage": "https://github.com/CodogoFreddie/hyperpipe",
	"bugs": "https://github.com/CodogoFreddie/hyperpipe",
	"keywords": [
		"hyperswarm",
		"hyperpipe",
		"hyper",
		"dht",
		"stdin",
		"stdout",
		"distributed"
	],
	"bin": {
		"hyperpipe": "./main.js"
	},
	"scripts": {
		"build": "babel src --out-dir lib",
		"format": "prettier --write src/**/*",
		"watch": "babel src --out-dir lib --watch"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-eslint": "^10.0.1",
		"babel-preset-freddie-ridell": "0.0.8",
		"eslint": "^5.9.0",
		"eslint-config-react-app": "^3.0.5",
		"eslint-plugin-flowtype": "^3.2.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-jsx-a11y": "^6.1.2",
		"eslint-plugin-react": "^7.11.1",
		"prettier": "^1.15.3"
	},
	"babel": {
		"presets": [
			"freddie-ridell"
		]
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"files": [
		"/index.js",
		"/lib",
		"/main.js"
	],
	"prettier": {
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true
	},
	"dependencies": {
		"@hyperswarm/network": "0.0.4"
	}
}
